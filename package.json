{
  "name": "@ngx-pwa/local-storage",
  "version": "8.2.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "start:iframe": "ng serve iframe --port 8080",
    "build": "ng build ngx-pwa-local-storage --prod && copyfiles README.md LICENSE dist/ngx-pwa/local-storage && npm run schematics",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "ng test ngx-pwa-local-storage --watch false",
    "test:e2e": "npm run webdriver && npm run test:e2e:demo && npm run test:e2e:interoperability",
    "test:e2e:demo": "ng e2e demo",
    "test:e2e:interoperability": "ng e2e localforage",
    "test:ivy": "ng e2e ivy",
    "webdriver": "webdriver-manager update",
    "lint": "ng lint",
    "schematics": "tsc -p projects/ngx-pwa/local-storage/tsconfig.schematics.json && copyfiles --up 3 projects/ngx-pwa/local-storage/schematics/collection.json projects/ngx-pwa/local-storage/schematics/migration.json projects/ngx-pwa/local-storage/schematics/**/schema.json dist/ngx-pwa/local-storage",
    "publish": "npm run lint && npm run build && npm test && npm publish dist/ngx-pwa/local-storage --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/router": "8.2.4",
    "rxjs": "6.5.2",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.3",
    "@angular-devkit/build-ng-packagr": "0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "8.2.4",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.14.17",
    "codelyzer": "5.1.0",
    "copyfiles": "2.1.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.2.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-safari-launcher": "1.0.0",
    "karma-safari-private-launcher": "1.0.0",
    "localforage": "1.7.3",
    "ng-packagr": "5.5.0",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tsickle": "0.37.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "zone.js": "0.10.2"
  }
}
